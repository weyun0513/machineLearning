   /* Load a data set */  File myFile = new File("C:/lottery");    String csvFile = "C:/abc.csv";   FileWriter writer = new FileWriter(csvFile);  FileInputStream fis = new FileInputStream(myFile);   // Finds the workbook instance for XLSX file  BufferedReader br=new BufferedReader(new FileReader(myFile));      String line_str=br.readLine();     String date="";     String[] winData=new String[6];     int winNum=0;     int whichDay=0;    while(line_str!=null&& ! line_str.equals("")){     winNum=0;     winData=line_str.split(",");     date=winData[0];     for(int i=1;i<40;i++){            if(Arrays.asList(winData).contains(i))       winNum=1;      CSVUtils.writeLine(writer, Arrays.asList(date,String.valueOf(i),String.valueOf(winNum)));           }    }
